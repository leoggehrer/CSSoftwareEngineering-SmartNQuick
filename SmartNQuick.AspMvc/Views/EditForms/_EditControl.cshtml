@*@Ignore*@
@{
    @model SmartNQuick.AspMvc.Models.Persistence.Test.EditForm

    if (Model.ViewBagInfo.DisplayProperty.Name.Equals("CheckBox"))
    {
        Model.ViewBagInfo.Handled = true;
        <input type="checkbox" asp-for="CheckBox" />
    }
    else
    {
        @await Html.PartialAsync("../Shared/_EditControl", Model)
    }
}