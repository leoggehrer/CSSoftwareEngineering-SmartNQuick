@*@BaseCode*@
@{
    @using SmartNQuick.AspMvc.Modules.View
    @model SmartNQuick.AspMvc.Models.IdentityModel

    var viewBagWrapper = Model.ViewBagInfo;
    var property = viewBagWrapper.DisplayProperty;
    var displayViewModel = ViewModelCreator.CreateDisplayViewModel(viewBagWrapper, Model);

    @displayViewModel.GetDisplayValue(property)
    viewBagWrapper.Handled = true;
}